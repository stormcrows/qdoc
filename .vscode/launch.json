    "version": "0.2.0",
    "configurations": [
        {
            "name": "Search",
            "type": "go",
            "request": "launch",
            "trace": "verbose",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {},
            "args": ["search", "books/", "knight"]
        },
        {
            "name": "Serve",
            "type": "go",
            "request": "launch",
            "trace": "error",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {},
            "args": ["serve", "books/", "9999", "-i"]
        }
    ]
}